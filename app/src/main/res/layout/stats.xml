<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondFragment">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="76dp"
        android:layout_height="20dp"
        android:text="Pie charts"
        app:layout_constraintEnd_toEndOf="@+id/textureView2"
        app:layout_constraintStart_toStartOf="@+id/textureView2"
        tools:layout_editor_absoluteY="378dp" />

    <TextureView
        android:id="@+id/textureView2"
        android:layout_width="176dp"
        android:layout_height="115dp"
        android:layout_marginTop="296dp"
        android:foregroundTint="#FAFAFA"
        android:hapticFeedbackEnabled="true"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="219dp" />

    <TextureView
        android:id="@+id/textureView"
        android:layout_width="176dp"
        android:layout_height="115dp"
        android:layout_marginTop="296dp"
        android:foregroundTint="#FAFAFA"
        android:hapticFeedbackEnabled="true"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="77dp"
        android:layout_height="69dp"
        android:src="@drawable/ic_baseline_pie_chart_24"
        app:layout_constraintEnd_toEndOf="@+id/textureView2"
        app:layout_constraintStart_toStartOf="@+id/textureView2"
        tools:layout_editor_absoluteY="300dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="76dp"
        android:layout_height="20dp"
        android:text="Bar charts"
        app:layout_constraintEnd_toEndOf="@+id/textureView"
        app:layout_constraintStart_toStartOf="@+id/textureView"
        tools:layout_editor_absoluteY="377dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="77dp"
        android:layout_height="69dp"
        android:src="@drawable/ic_baseline_bar_chart_24"
        app:layout_constraintEnd_toEndOf="@+id/textureView"
        app:layout_constraintStart_toStartOf="@+id/textureView"
        tools:layout_editor_absoluteY="300dp" />

</androidx.constraintlayout.widget.ConstraintLayout>